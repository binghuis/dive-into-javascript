   "source": [
    "function* generator(i) {\n",
    "  yield i;\n",
    "  yield i + 10;\n",
    "}\n",
    "\n",
    "const gen = generator(10);\n",
    "\n",
    "console.log(gen.next());\n",
    "console.log(gen.next());\n",
    "console.log(gen.next());\n",
    "console.log(gen.next());\n"
   ]
  }
